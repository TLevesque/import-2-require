{
  "name": "import-to-require",
  "displayName": "Import To Require Syntax",
  "description": "Convert ES6 module import syntax to ES5 require syntax",
  "version": "0.6.0",
  "publisher": "tlevesque",
  "repository": "https://github.com/TLevesque/import-2-require.git",
  "engines": {
    "vscode": "^1.27.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.replaceAllImports",
    "onCommand:extension.replaceAllExports"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.replaceAllImports",
        "title": "Replace 'import' with 'require'"
      },
      {
        "command": "extension.replaceAllExports",
        "title": "Replace 'export' with CommonJS export"
      }
    ],
    "keybindings": [
      {
        "command": "extension.replaceAllImports",
        "key": "ctrl+alt+m",
        "mac": "ctrl+alt+m",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.replaceAllExports",
        "key": "ctrl+alt+p",
        "mac": "ctrl+alt+p",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
